{
  "name": "scanned-pdf-to-text-extractor",
  "version": "1.0.0",
  "description": "Servicio de extracción de texto desde PDFs escaneados y documentos de imagen utilizando múltiples motores: Ollama Vision, OpenAI Vision y Tesseract OCR",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "watch": "tsc -w"
  },
  "author": "ddarthp",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "ollama": "^0.4.4",
    "openai": "^4.85.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.1.1",
    "pdfjs-dist": "^3.11.174",
    "sharp": "^0.33.2",
    "tesseract.js": "^4.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19",
    "@types/pdf-parse": "^1.1.4",
    "@types/sharp": "^0.32.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
